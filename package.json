{
  "name": "node-typescript-boilerplate",
  "version": "0.0.0",
  "description": "Minimalistic boilerplate to quick-start Node.js development in TypeScript.",
  "engines": {
    "node": ">= 14.16 <15"
  },
  "main": "dist/server.js",
  "scripts": {
    "prestart": "npm run lint --fix && npm run build",
    "start": "node dist/server.js",
    "client": "node dist/client",
    "health": "node dist/health",
    "clean": "rimraf coverage build tmp",
    "build": "rimraf dist && tsc -p tsconfig.release.json",
    "build:proto": "node scripts/proto",
    "build:watch": "tsc -w -p tsconfig.release.json",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "author": "Jakub Synowiec <github@jakubsynowiec.info>",
  "license": "Apache-2.0",
  "dependencies": {
    "@grpc/grpc-js": "^1.3.2",
    "google-protobuf": "^3.17.2",
    "tslib": "~2.2.0"
  },
  "devDependencies": {
    "@types/google-protobuf": "^3.15.2",
    "@types/jest": "~26.0.23",
    "@types/node": "~14.14.44",
    "@typescript-eslint/eslint-plugin": "~4.23.0",
    "@typescript-eslint/parser": "~4.23.0",
    "eslint": "^7.27.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "~8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "~24.3.6",
    "eslint-plugin-sonarjs": "^0.7.1-117",
    "grpc-tools": "^1.11.1",
    "grpc_tools_node_protoc_ts": "^5.2.2",
    "jest": "~26.6.3",
    "prettier": "~2.3.0",
    "protobufjs": "^6.11.2",
    "rimraf": "~3.0.2",
    "shelljs": "^0.8.4",
    "ts-jest": "~26.5.6",
    "ts-node": "^10.0.0",
    "tsutils": "~3.21.0",
    "typescript": "~4.2.4"
  },
  "volta": {
    "node": "14.16.1"
  }
}
